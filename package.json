{
  "name": "11ty_intro",
  "version": "1.0.0",
  "description": "",
  "author": "Simon Pinkas",
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "dev:eleventy": "npx @11ty/eleventy --serve",
    "dev:parcel": "parcel watch assets/img/* assets/css/* assets/js/* --dist-dir _site/assets",
    "build": "run-s prod:*",
    "prod:eleventy": "ELEVENTY_ENV=production eleventy --output=./build",
    "prod:parcel": "parcel build assets/images/*  assets/css/* assets/js/* --dist-dir build/assets"
  },
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-img": "^2.0.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.4.0"
  }
}
